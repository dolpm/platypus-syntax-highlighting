{
  "name": "platypus-syntax-highlighting",
  "displayName": "platypus-syntax-highlighting",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
